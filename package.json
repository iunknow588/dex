{
  "name": "luckengine-dex",
  "displayName": "智运通交易系统 (LuckEngine)",
  "version": "1.0.0",
  "description": "基于 Injective 区块链的去中心化交易所，提供三大交易模式：低风险合约交易、无风险套利交易、低风险闪电贷模式",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production vite build --mode production",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "size-check": "node scripts/monitor-project-size.js",
    "deploy": "bash scripts/deploy.sh",
    "deploy:vercel": "bash scripts/deploy-vercel.sh",
    "deploy:preview": "npm run build && vercel",
    "deploy:prod": "npm run build:prod && vercel --prod",
    "vercel:build": "npm run build:prod",
    "start": "vite preview --port 3000 --host 0.0.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.36.2",
    "@cosmjs/proto-signing": "^0.36.2",
    "@cosmjs/stargate": "^0.36.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@injectivelabs/sdk-ts": "^1.16.22",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@tanstack/react-query": "^5.90.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2"
  }
}
